{
  "name": "deputydex-data",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "first-setup": "npm run docker:db && npm run prisma:generate && npm run prisma:migrate-dev && npm run prisma:seed",
    "docker:db": "ts-node local/db/local-db-init.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate-dev": "dotenv -e .env.local -- npx prisma migrate dev",
    "prisma:migrate-prod": "prisma migrate deploy",
    "prisma:seed": "ts-node --project tsconfig.json prisma/seed.ts",
    "prisma:pull": "prisma db pull",
    "prisma:reset": "dotenv -e .env.local -- npx prisma migrate reset"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "adm-zip": "^0.5.16",
    "dotenv": "^17.2.3",
    "pg": "^8.18.0",
    "unzipper": "^0.12.3"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/node": "^25.2.0",
    "@types/pg": "^8.16.0",
    "dotenv-cli": "^11.0.0",
    "prisma": "^7.3.0",
    "typescript": "^5.5.3"
  },
  "private": true
}
